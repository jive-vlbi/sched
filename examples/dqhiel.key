         701.75,701.75, 717.75,717.75     /
endset /


setini = band.1cm / 
  dbe = rdbe_ddc  netside= U  sideband= U
  bbfilt= 16      nchan= 8    pol= dual    bits= 2
  firstlo= 21500  
  bbsyn= 703.75, 703.75, 719.75,719.75, 735.75,735.75, 751.75, 751.75  /
endset /


setini = band.7mm / 
  dbe = rdbe_ddc  netside= U  sideband= U
  bbfilt= 16      nchan= 8    pol= dual    bits= 2
  firstlo= 42400  
  bbsyn= 703.75, 703.75, 719.75,719.75, 735.75,735.75, 751.75, 751.75  /
