lst='vlba_pt'   ! dynamic scheduling.

! ------ This observation ------

expcode = 'DQHIEL'    version = 1
day = 63521 
start = 23:03:00
stations =  SC, HN, NL, FD, LA, PT, KP, OV, BR, MK


! ------ ------ ------ ------ ------ ------ ------ ------
