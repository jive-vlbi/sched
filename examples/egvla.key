! -------------------------------------------------------------------
! EXAMPLE of SCHED file for non-VLBI VLA observations.
! -------------------------------------------------------------------
!  VLA observations to get flux densities of DA193 and 3C84 at VLA.
!  Note that the example setup files are used for cc and qq bands.

! -----  Cover Information
overwrit

version  = 1 
expt     = 'Image 20 arcsec knot for comparison with Xrays'
piname   = 'Craig Walker'    
address1 = 'National Radio Astronomy Observatory'  
address2 = 'P. O. Box O'                           
address3 = 'Socorro, New Mexico, 87801'
address4 = ' U.S.A. '
phone    = '505 835 7247 '    
obsphone = '505 835 7247 '     
email    = 'cwalker@nrao.edu'  
fax      = '505 835 7027 '   
obsmode  = 'Continuum'
correl   = 'Socorro'           

note1    = 'Set delays to 0.5 ns during the second (VX mode) scan'

! -----  File names and other details.
 
stafile   = $SCHED/catalogs/stations.dat
srcfile   = $SCHED/catalogs/sources.vlba
obstype   = vla           ! Tells SCHED that this is VLA only.
expcode   = AH672
stations  = VLA
vlatype   = CONTINUUM
vlausern  = 450

setinit = vla-uu.set /   ! copy of standard non-VLBI file.
  station  = VLA
  format   = none
  vlaband  = UU
  vlabw    = '0000' /
endset /
setup = vla-uu.set

! ----  The schedule.

lst                       ! Schedule in LST.

day = 58260               ! LST day of stop of first scan.
start = 22:30:00           ! LST start time.

vlamode = 'VA'
source  = '3C84'  dur = 15:00     /
vlamode = 'VX'
comment = 'Set delays to 0.5 ns. '
source  = '3C84'  dur = 28:00     /
comment = 'Skip to this scan once delays are set.
vlamode = 'VA'
source  = '3C84'  dur = 2:00      /
vlamode = 'VX'
source  = '3C48'  dur = 10:00     /

vlamode = 'VA'  group = 2 rep = 10
source  = '3C120' dur = 54:00     /
source  = '3C84'  dur = 10:00     /

vlamode = 'VX'
source  = '3C286' stop = 10:30:00 day = 58261 dur = 0      /

