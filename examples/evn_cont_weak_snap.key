endcat /

! ==========================================================
! ==================== Setup Information ===================
! ==========================================================

! Get frequency information from the DEFAULT catalog

freqfile = '$SCHED/catalogs/freq.dat'

! Construct frequency setup
! 8 8-MHz channels, 2-bit sampling and dual polarisation => 256 Mb/s

setinit='eo999.6cm' /
        band='6cm'
        nchan=8
        bbfilter=8
        bits=2
        pol='dual'
        pcal='1MHz'
        barrel=roll_off /
endset /

