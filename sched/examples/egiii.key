!   Example sched input file for Mark III observations on the VLBA
!
! ==========================================================
! =================  Cover Information  ====================
! ==========================================================
!
version  = 1
expt     = 'egiii MkIII mode B dbl spd 18&4cm 09oct94 12:00-24:00 ut Wrobel'
expcode  = 'egiii'
piname   = 'J.M. Wrobel'
address1 = 'NRAO, P.O. Box O'
address2 = 'Socorro, NM 87801'
address3 = 'USA'
phone    = '+1-505-835-7392 (work) or +1-505-835-3972 (home)'
obsphone = '+1-505-835-7392 (work) or +1-505-835-3972 (home)'
fax      = '+1-505-835-3972'
email    = 'jwrobel@nrao.edu (internet)'
obsmode  = 'MkIII mode B double speed 18&4cm Network standards'
obstype  = 'mkiii'
note1    = 'Please use all available antennas.'
note2    = 'Fringe finder, manual pulse cal: J1229+02 (3C273)'
note3    = 'Amplitude check                : J1310+32, J1407+28 (OQ208)'
!
! ==========================================================
! ==============  Correlator Information  ==================
! ==========================================================
 
correl   = 'Socorro'
coravg   = 4
corchan  = 16
cornant  = 10
corpol   = 'on'
corwtfn  = 'uniform'
corsrcs  = 'standard'
cortape  = DAT
corship1 = 'Craig Walker'
corship2 = 'P. O. Box O'
corship3 = 'Socorro NM 87801'

! ==========================================================
! ==================  Program control  =====================
! ==========================================================
!
!    Normally SCHED will not overwrite preexisting files.  If
!    you do not want this protection, uncomment the following line:
 overwrit
!
!    To only process VLBA and VLA stations, uncomment the following line:
! dosta 'vl'
!
! ==========================================================
! =================== Tape initialization  =================
! ==========================================================
!**  WARNING: Check with data analysts that the following tape  **
!**           information is correct for YOUR project!          **
!    For VLBA and Global observations going to the VLBA correlator,
!    all stations must use thin, high density tapes.  For this,
!    the defaults can be used and this section can be omitted.
!    For EVN or other observations going to other correlators,
!    this section may be needed to specify thick tapes.
!
tapeini /
  !  Thin and thick tapes are 17600 and 8800 ft long respectively.
  tpsta   = vlba,   vla,    n140vlba, eb_vlba, default
  tpdrive = 1,      1,      1,       1,       1
  tplength= 17600,  17600,  8800,    8800,    8800
  ndrives = 2,      2,      1,       1,       1
  nheadpos= 14,     14,     14,      14,      14
  density = H,      H,      L,       L,       L
   /
!
! ==========================================================
! ====================  Source Catalog  ====================
! ==========================================================
!
!  Positions for sources not in the statndard catalog.
srccat /
 source='M84'      RA=12:25:03.732 DEC= 12:53:13.31 EQUINOX='J2000' CALCODE=' ' /
 source='NGC4374'  RA=12:25:03.732 DEC= 12:53:13.31 EQUINOX='J2000' CALCODE=' ' /
 source='UGC07494' RA=12:25:03.732 DEC= 12:53:13.31 EQUINOX='J2000' CALCODE=' ' /
 source='3C272.1'  RA=12:25:03.732 DEC= 12:53:13.31 EQUINOX='J2000' CALCODE=' ' /
endcat /
!
!  Standard catalog.
!
srcfile  = $SCHED/catalogs/sources.vlba
!
! ==========================================================
! ====================  Station Catalog  ===================
! ==========================================================
!
stafile  = $SCHED/catalogs/stations.dat
!
! ==========================================================
! =================  Initial Scan Information  =============
! ==========================================================
!
stations = VLBA_SC, VLBA_HN, VLBA_NL, VLBA_FD, VLBA_LA,
           VLBA_PT, VLBA_KP, VLBA_OV, VLBA_BR, VLBA_MK
year     = 1994
month    = 10
day      = 9
start    = 13:03:30
!
! ==========================================================
