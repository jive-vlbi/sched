SOME GEODESY MODES.

#mode 64-8-1
  @mode spd4
!      Useful for low tape consumption geodesy.
!      64 Mbps - 2 pass/headpos
  nchan    = 8 
  bits     = 1
  bbfilter = 4.0  
  #vformat format = VLBA1:2
  #4format format = MKIV1:2
  @mode shell
#end

#mode 256-8-1
!     Only for geodesy modes.  16 MHz channels.
  @mode spd8
!      256 Mbps - 1 pass/headpos
  nchan    = 8 
  bits     = 1 
  bbfilter = 16.0
  #vformat format = VLBA1:4
  #4format format = MKIV1:4
  @mode shell
  #end



#file vgeo-64-8-1.set    @mode 64-8-1   @mode sxg  @mode rcp_g
  #foff  0.0, 10.0, 70.0, 100.0, 0.0, 80.0, 260.0, 400 
#end  

#file vgeo-128-8-1.set   @mode 128-8-1  @mode sxg  @mode rcp_g
  #foff  0.0, 10.0, 70.0, 100.0, 0.0, 80.0, 260.0, 400.0
#end  

#file vgeo-256-8-1.set   @mode 256-8-1  @mode sxg  @mode rcp_g
!       Come up with a better frequency sequence.  Note Ch 1&2 overlap!
!       Try for a 3 S, 5 X sequence that spans more bandwidth.
  #foff  0.0, 10.0, 70.0, 100.0, 0.0, 80.0, 260.0, 400.0
#end  

#file m3geosx.set
!   Normal geodesy mode without frequency switching.  2 pass/headpos
!   Have to force tpmode or SCHED will set 4.
  format   = MARKIII
  #vformat
  #4format
  tpmode   = 2
  nchan    = 8
  bits     = 1
  bbfilter = 2.0  
  pol      = rcp
  netside  = U, U, U, U, U, U, U, U
  bbc      = 1, 2, 3, 4, 5, 6, 7, 8
  freqref  = 8210.99, 8250.99, 8310.99, 8570.99,
             2222.99, 2237.99, 2292.99, 2302.99
  track1  = 18,  20,  6, 10, 12, 28, 30, 16
  track2  = 19,  21,  7, 11, 13, 29, 31, 17
   /
#end

#file m3geord.set
  !   Setup file for NASA R&D observations.  
  !   This is a frequency switched setup.
  !   Attach dummy channels to unswitched BBC other sidebands.
  !
  !  HEADER FROM CDP SCHEDULE:
  !*
  !* Chan. Sky fr. Syn. BBC    BBC IF Track  Track  BBC# Set    For VLBA
  !*       MKIII        freq   SB     (For)  (Rev)  VLBA        set to
  !* ----------------------------------------------------------
  !*  1  8212.99  7600 612.99   U   B  18     19     1   All
  !*  2  8252.99  7600 652.99   U   B   4      5     2   Set1
  !*  3  8352.99  7600 752.99   U   B  20     21     2   Set2
  !*  4  8512.99  7600 912.99   U   B   6      7     2   Dummy  Ch 1, LSB
  !*  5  8732.99  9600 867.01   L   D  22     23     3   Set2
  !*  6  8852.99  9600 747.01   L   D   8      9     3   Dummy  Ch 8, LSB
  !*  7  8912.99  9600 687.01   L   D  24     25     3   Set1
  !*  8  8932.99  9600 667.01   L   D  10     11     4   All
  !* ----------------------------------------------------------
  !*  9  2220.99  2900 679.01   L   A  26     27     5   All
  !* 10  2230.99  2900 669.01   L   A  12     13     6   all
  !* 11  2250.99  2900 649.01   L   A  28     29     6   dummy  Ch 9, LSB
  !* 12  2305.99  2900 594.01   L   A  14     15     7   Set2
  !* 13  2340.99  2900 559.01   L   A  30     31     7   Set1
  !* 14  2345.99  2900 554.01   L   A  16     17     8   All
  !* ----------------------------------------------------------
  
  nchan = 14  samprate=8.0  bits = 1  bbfilter = 4.0
  tpmode = 2  format = MARKIII
  fe ='13cm', '4cm', '13cm', '4cm'    synth = 7.6, 2.9, 9.6
  dualx  frswitch  swtchdur=15
  firstlo = 7600,   7600,   7600,   7600,   9600,   9600,   9600,   9600,
            2900,   2900,   2900,   2900,   2900,   2900
  bbsyn   =  612.99, 652.99, 652.99, 612.99, 687.01, 667.01, 687.01,
             667.01, 679.01, 669.01, 679.01, 559.01, 559.01, 554.01
  bbsyn2  =  612.99, 752.99, 752.99, 612.99, 867.01, 667.01, 867.01,
             667.01, 679.01, 669.01, 679.01, 594.01, 594.01, 554.01
  sideband=  U,  U,  U,  L,  L,  U,  L,  L,  L,  L,  U,  L,  L,  L
  netside =  U,  U,  U,  L,  U,  L,  U,  U,  U,  U,  L,  U,  U,  U
  ifchan  =  B,  B,  B,  B,  D,  D,  D,  D,  A,  A,  A,  A,  A,  A
  bbc     =  1,  2,  2,  1,  3,  4,  3,  4,  5,  6,  5,  7,  7,  8
  track1  = 18,  4, 20,  6, 22,  8, 24, 10, 26, 12, 28, 14, 30, 16
  track2  = 19,  5, 21,  7, 23,  9, 25, 11, 27, 13, 29, 15, 31, 17

  #vformat
  #4format
    /
#end



VSOP Setups

#mode VSOP
!      128 Mbps - 2 pass/headpos
  @mode spd8
  nchan    = 2 
  bits     = 2 
  bbfilter = 16.0
  pol      = lcp
  freqref  = @freq
  netside  = U, U
#end 


#file vsop18cmvla.set  
!  This is the file for experiments involving the VLA and not Jodrell.
!  VSOP covers 1.60 - 1.73 GHz 
  #freq  1640.0, 1656.0
  @mode VSOP  #end


#file vsop18cmjod.set
!  This is the file for experiments involving Jodrell and not the VLA.
!  ****  DO NOT USE FOR THE VLA WITHOUT SPECIAL PERMISSION
!  ****  CONTACT JOAN WROBEL.  SPECIAL CABLES REQUIRED.
!  VSOP covers 1.60 - 1.73 GHz 
  #freq  1634.0, 1650.0
  @mode VSOP  #end


#file vsop6cm.set
!  VSOP covers 4.7 - 5.0 GHz  
!  Stay above 4960.1 for VLA VC mode.
  #freq 4962.00,  4978.00
  @mode VSOP  #end


#file vsop1cm.set 
!  VSOP covers 22.0 - 22.3 GHz
  #freq 22200.0, 22216.0     
  @mode VSOP  #end





PULSE CAL TEST FILES

#mode pcu
!         pcal tests, upper sidebands, 1 bit.
!         VLBA format needed to set up formatter.
  station  = VLBA
  format   = VLBA
  nchan    = 8
  bits     = 1
  bbfilter = 1.0 
  bbc      = 1, 2, 3, 4, 5, 6, 7, 8
  netside  = U, U, U, U, U, U, U, U
  pol      = RCP, RCP, RCP, RCP, LCP, LCP, LCP, LCP
  freqoff  = -1.5, -0.5, 0.5, 1.5, -1.5, -0.5, 0.5, 1.5
#end

#file pc90cm.set  @mode pcu  freqref = 325.99    /  #end
#file pc50cm.set  @mode pcu  freqref = 607.99    /  #end
#file pc18cm.set  @mode pcu  freqref = 1662.99   /  #end  
#file pc13cm.set  @mode pcu  freqref = 2266.99   /  #end  
#file pc6cm.set   @mode pcu  freqref = 4990.99   /  #end  
#file pc4cm.set   @mode pcu  freqref = 8416.99   /  #end  
#file pc2cm.set   @mode pcu  freqref = 15360.99  /  #end  
#file pc1cm.set   @mode pcu  freqref = 22228.99  /  #end  
#file pc7mm.set   @mode pcu  freqref = 43212.99  /  #end  

#mode pcl
!         pcal tests, lower sidebands, 1 bit.
!         VLBA format needed to set up formatter.
  station  = VLBA
  format   = VLBA 
  nchan    = 8
  bits     = 1 
  bbfilter = 1.0
  bbc      = 1, 2, 3, 4, 5, 6, 7, 8
  netside  = L, L, L, L, L, L, L, L
  pol      = RCP, RCP, RCP, RCP, LCP, LCP, LCP
  freqoff  = -1.5, -0.5, 0.5, 1.5, -1.5, -0.5, 0.5, 1.5
#end

#file pcl6cm.set  @mode pcl  freqref = 4990.99  /  #end  
#file pcl4cm.set  @mode pcl  freqref = 8416.99  /  #end  


#mode pc2
!         pcal tests, upper sidebands, 2 bit.
!         VLBA format needed to set up formatter.
  station  = VLBA
  format   = VLBA  
  nchan    = 4 
  bits     = 2 
  bbfilter = 1.0 ! pcal tests
  pol      = RCP, LCP, RCP, LCP
!        SCHED does not automatically request 2 bit tone detections.
  pcalxb1  = S1,   S2,  S3,  S4,  S1,  S2,  S3,  S4
  pcalxb2  = M1,   M2,  M3,  S4,  M1,  M2,  M3,  S4
  pcalfr1  = 510, 510, 510, 510,   0,   0,   0,   0
  pcalfr2  = 510, 510, 510, 510,   0,   0,   0,   0
  #end
#mode pc2u
  bbc      = 1, 2, 3, 4 
  netside  = U, U, U, U
  freqoff  = -0.5, -0.5, 0.5, 0.5   
#end
#mode pc2l
  bbc      = 1, 2, 3, 4 
  netside  = L, L, L, L  
  freqoff  = -0.48, -0.48, 0.52, 0.52 
#end
                                                    
#file pc2a6cm.set  @mode pc2  @mode pc2u  freqref = 4990.99  /  #end  
#file pc2a4cm.set  @mode pc2  @mode pc2u  freqref = 8416.99  /  #end  
#file pc2b6cm.set  @mode pc2  @mode pc2l  freqref = 4990.99  /  #end  
#file pc2b4cm.set  @mode pc2  @mode pc2l  freqref = 8416.99  /  #end  
#file pc2c6cm.set  @mode pc2  @mode pc2u  freqref = 4990.99  /  #end  
#file pc2c4cm.set  @mode pc2  @mode pc2u  freqref = 8416.99  /  #end  
#file pc2d6cm.set  @mode pc2  @mode pc2l  freqref = 4990.99  /  #end
#file pc2d4cm.set  @mode pc2  @mode pc2l  freqref = 8416.99  /  #end  


MARK III MODE B

#mode m3b
  @mode spd4
!      56 Mbps  -  2 passes per head position.
  nchan    = 14  
  bits     = 1
  bbfilter = 2.0
  format   = MARKIII
  #vformat 
  #4format 
  @mode shell
#end

#file m3b90cm.set  !  Not in codes.sch.  Not standard.
                   @mode 90cm  #f1 -4.0;  @mode lcp_ul  @mode m3b  #end
#file m3b50cm.set  @mode 50cm  #f1 -22.0; 
  lcp50cm  = 'BROAD' rcp50cm = 'BROAD'    @mode lcp_ul  @mode m3b  #end
#file m3b18cm.set  @mode 18cm  #f1 -8.0;  @mode lcp_ul  @mode m3b  #end
#file m3b13cm.set  !   Related to Mode A spec in Tony Foley email of 17 Nov 1995.
                   @mode 13cm  #f1 -34.0; @mode rcp_ul  @mode m3b  #end
#file m3b6cm.set   @mode 6cm   #f1 -16.0; @mode lcp_ul  @mode m3b  #end
#file m3b4cm.set
!     Not standard.
!     Network standard suggested by Tony Foley in email of 17 Nov 1995
!     is closer to 8300 MHz, not 8400.  This is not in the deep space band.
                   @mode 4cm   #f1 -12.0; @mode rcp_ul  @mode m3b  #end
#file m3b2cm.set  
!     Not in codes.sch - not Mark III standard.
                   @mode 2cm   #f1 -12.0; @mode lcp_ul  @mode m3b  #end
#file m3b1cm.set   @mode 1cm   #f1 -4.0;  @mode lcp_ul  @mode m3b  #end
#file m3b7mm.set  !      Not in codes.sch - not Mark III standard.
                   @mode 7mm   #f1 -12.0; @mode lcp_ul  @mode m3b  #end


DOUBLE SPEED MARK III MODE B

