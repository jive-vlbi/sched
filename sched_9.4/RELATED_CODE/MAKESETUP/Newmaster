MARKIII MODE B POLARIZATION  (FOR USE WITH VLBA)

#mode m3cpol
  @mode spd4
!      32 Mbps  -  2 passes per head position
!      Should set up for MARK III/IV stations as VEX becomes ready.
  nchan    = 8
  bits     = 1
  bbfilter = 2.0
  tpmode   = 2       ! This has to be forced.
  bbc      = 1,  2,  3,  4,  5,  6,  7,  8
  track1   = 20,  6, 22,  8, 24, 10, 26, 12
  track2   = 21,  7, 23,  9, 25, 11, 27, 13
  #side  U,  U,  U,  U,  U,  U,  U,  U
  #pol   LCP, RCP, LCP, RCP, LCP, RCP, LCP, RCP
  format   = MARKIII
  #vformat 
  #4format 
  @mode shell
#end

#file m3cpol18cm.set  !  Not in codes.sch.  Not standard.
  @mode 18cm  @mode m3cpol 
  #foff  -4, -4, -2, -2, 0, 0, 2, 2;   #end

#file m3cpol6cm.set  @mode 6cm  @mode m3cpol  
  #foff  -11, -11, -9, -9, -7, -7, -5, -5;   #end

#file m3cpol4cm.set   @mode 4cm  @mode m3cpol 
  #foff  -7, -7, -5, -5, -3, -3, -1, -1;   #end



MARK III MODE A POLARIZATION  (FOR USE WITH THE VLBA IN MODE B)

#mode m3abpol
!  ******   WARNING - this mode cannot be correlated on the VLBA correlator.
!         The track assignments are incompatible with full stokes processing.
!      Mark III polarization to use with psuedo mode A at Mark III sites.
!      Channels not in ascending frequency order with these track assignments. 
!      Should set up for MARK III/IV stations as VEX becomes ready.
  @mode spd4
!      48 Mbps  --  Two passes per head position on VLBA
  nchan    = 12 
  bits     = 1
  bbfilter = 2.0
  tpmode   = 2  
  bbc      =  1,  2,  3,  4,  5,  6,  1,  2,  3,  4,  5,  6
  track1   =  4,  5,  6,  7,  8,  9, 18, 19, 20, 21, 22, 23
  track2   = 10, 11, 12, 13, 14, 15, 24, 25, 26, 27, 28, 29
  #side  U,  U,  U,  U,  U,  U,  L,  L,  L,  L,  L,  L
  #pol   RCP,LCP,RCP,LCP, RCP,LCP,RCP,LCP, RCP,LCP,RCP,LCP
  format   = MARKIII
  #vformat 
  #4format 
  @mode shell
#end

#file m3abpol6cm.set  
!      Should set up for MARK III/IV stations as VEX becomes ready.
  @mode 6cm  @mode m3abpol  
  #foff -9, -9, -5, -5, -1, -1, -9, -9, -5, -5, -1, -1;  #end

#file m3abpol4cm.set   @mode 4cm  @mode m3abpol 
  #foff -5, -5, -1, -1, 3, 3, -5, -5, -1, -1, 3, 3 ;  #end




SPECIAL SETUP FOR ALL 4 OH LINES

#file vOH.set
!         Setup for VLBA observations of all OH lines.
!         18 cm wavelength  4X oversampling.
!         32 Mbps - 2 pass/headpos
  station  = VLBA
  format   = 'VLBA1:1'
  nchan    = 8 
  samprate = 2.0
  bits     = 2 
  bbfilter = 0.250
  freqref  =  1612.11,1612.11, 1665.28,1665.28, 1667.22,1667.22, 1720.41,1720.41
  netside  =       U,      U,       U,      U,       U,      U,       U,      U
  bbc      =       1,      2,       3,      4,       5,      6,       7,      8
  pol      = dual
  pcal     = 'off'
  /
!  Uncomment for stations with VLBA recorders and VEX control
!  format   = 'VLBA1:1'
!  station  = 
! /
!  Uncomment for stations with MarkIV recorders and VEX control
!  format   = 'MKIV1:1'
!  station  = 
! /

!          VLA can only do 2 center lines.  Make 2 copies of each.
  station  = VLA1, VLA27
  freqref  = 1665.28,1665.28, 1665.28,1665.28, 1667.22,1667.22, 1667.22,1667.22
  vlaband = VL   vlabw = '0000'   /   #end




SPECIAL SETUP FOR METHANOL AT 12 GHz

#file vmeth.set
!          SCHED setup file for Methanol 12 GHz observations.
!          Just over 24 hours recording at 13 minutes per pass.
!          16 Mbps - 8 pass/headpos  
  station  = VLBA
  format   = VLBA1:1
  nchan    = 2 
  bits     = 2
  bbfilter = 2.0
  freqref  = 12165.0
  netside  = U, U  
  pol      = dual
  bbc      = 1, 2
  pcal     = 'off'
  / 
!  Uncomment for stations with VLBA recorders and VEX control
!  format   = 'VLBA1:1'
!  station  = 
! /
!  Uncomment for stations with MarkIV recorders and VEX control
!  format   = 'MKIV1:1'
!  station  = 
! /
#end




